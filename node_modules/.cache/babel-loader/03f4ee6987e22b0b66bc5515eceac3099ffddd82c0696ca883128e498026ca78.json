ции и обеспечивать наличие базового случая, чтобы избежать бесконечных циклов.</p>\n<b>Пример</b>\n\n<pre>\n  <code>\n    {'function factorial(num) {\\n  if (num === 0 || num === 1) {\\n    return 1;\\n  } else {\\n    return num * factorial(num - 1);\\n  }\\n}\\n\\nvar result = factorial(5);\\nconsole.log(result); // Output: 120'}\n  </code>\n</pre>\n\n    </div>\n  )\n}\n\n\n\nexport default RuEs5_4;\n\n\n\n"],"mappings":";AAAA,OAAOA,KAAK,MAAM,OAAO;AAAA,SAAAC,MAAA,IAAAC,OAAA;AAExB,SAASC,OAAOA,CAAA,EAAG;EAClB,oBACED,OAAA;IAAAE,QAAA,gBACAF,OAAA;MAAAE,QAAA,EAAI;IAAQ;MAAAC,QAAA,EAAAC,YAAA;MAAAC,UAAA;MAAAC,YAAA;IAAA,QAAK,eACrBN,OAAA;MAAAE,QAAA,EAAG;IAAkO;MAAAC,QAAA,EAAAC,YAAA;MAAAC,UAAA;MAAAC,YAAA;IAAA,QAAI,eACzON,OAAA;MAAAE,QAAA,EAAG;IAAsL;MAAAC,QAAA,EAAAC,YAAA;MAAAC,UAAA;MAAAC,YAAA;IAAA,QAAI,eAC7LN,OAAA;MAAAE,QAAA,EAAG;IAA0F;MAAAC,QAAA,EAAAC,YAAA;MAAAC,UAAA;MAAAC,YAAA;IAAA,QAAI,eACjGN,OAAA;MAAAE,QAAA,eACEF,OAAA;QAAAE,QAAA,EACG;MAAoK;QAAAC,QAAA,EAAAC,YAAA;QAAAC,UAAA;QAAAC,YAAA;MAAA;IAChK;MAAAH,QAAA,EAAAC,YAAA;MAAAC,UAAA;MAAAC,YAAA;IAAA,QACH,eACNN,OAAA;MAAAE,QAAA,EAAG;IAA8O;MAAAC,QAAA,EAAAC,YAAA;MAAAC,UAAA;MAAAC,YAAA;IAAA,QAAI,eACrPN,OAAA;MAAAE,QAAA,EAAG;IAAoO;MAAAC,QAAA,EAAAC,YAAA;MAAAC,UAAA;MAAAC,YAAA;IAAA,QAAI,eAC3ON,OAAA;MAAAE,QAAA,EAAG;IAAM;MAAAC,QAAA,EAAAC,YAAA;MAAAC,UAAA;MAAAC,YAAA;IAAA,QAAI,eAEbN,OAAA;MAAAE,QAAA,eACEF,OAAA;QAAAE,QAAA,EACG;MAAwM;QAAAC,QAAA,EAAAC,YAAA;QAAAC,UAAA;QAAAC,YAAA;MAAA;IACpM;MAAAH,QAAA,EAAAC,YAAA;MAAAC,UAAA;MAAAC,YAAA;IAAA,QACH;EAAA;IAAAH,QAAA,EAAAC,YAAA;IAAAC,UAAA;IAAAC,YAAA;EAAA,QAEI;AAEV;AAACC,EAAA,GAxBSN,OAAO;AA4BjB,eAAeA,OAAO;AAAC,IAAAM,EAAA;AAAAC,YAAA,CAAAD,EAAA"},"metadata":{},"sourceType":"module","externalDependencies":[]}